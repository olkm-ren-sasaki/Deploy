<%= render 'books/user', user: current_user %>
<%= render 'books/book', book: @book %>

<h1>Users</h1>
<table>
 <tr>
  <th>image</th>
  <th>name</th>
 </tr>
<% @user.each do |user| %>
 <tr>
  <td><%= image_tag user.get_profile_image(100,100) %></td>
  <td><%= user.name %></td>
  <td><%= link_to "Show", user_path(user.id) %></td>
 </tr>
 <% end %>
</table>